  "version": "0.5.0",
  "description": "Collection of features and utility functions that can be used in most javascript projects",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "format": "eslint src/** --fix"
  },
